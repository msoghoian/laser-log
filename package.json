{
  "name": "laser-log",
  "private": true,
  "version": "0.0.0",
  "description": "User activity logging for the laser cutter",
  "author": "Matt Soghoian <mattsoghoian@gmail.com>",
  "type": "module",
  "main": "electron/main.js",
  "scripts": {
    "dev": "vite",
    "build:react": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "start:electron": "electron electron/main.js",
    "build": "npm run build:react && electron-builder"
  },
  "build": {
    "appId": "org.fresnoideaworks.laserlog",
    "productName": "Laser Log",
    "files": [
      "dist",
      "electron/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "mac": {
      "target": "dmg"
    },
    "win": {
      "target": "nsis"
    }
  },
  "dependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.6.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "electron": "^36.3.2",
    "electron-builder": "^26.0.12",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5"
  }
}
